{
  "expo": {
    "name": "Goals Manager",
    "slug": "goals-manager",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/places.png",
    "splash": {
      "image": "./assets/places-adaptive.png",
      "resizeMode": "cover",
      "backgroundColor": "#171717"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": ["VIBRATE"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
